{
    "name": "xcode-default-theme",
    "displayName": "Xcode Default Theme",
    "description": "Brings the colors of the Xcode 'Default (Dark)' and 'Default (Light)' themes to Visual Studio Code",
    "categories": [
        "Themes"
    ],
    "version": "2.0.7",
    "publisher": "smockle",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/smockle/xcode-default-theme.git"
    },
    "engines": {
        "vscode": "*"
    },
    "contributes": {
        "themes": [
            {
                "id": "Xcode Default (Dark)",
                "label": "Xcode Default (Dark)",
                "uiTheme": "vs-dark",
                "path": "./xcode-default-dark-theme.json"
            },
            {
                "id": "Xcode Default (Light)",
                "label": "Xcode Default (Light)",
                "uiTheme": "vs",
                "path": "./xcode-default-light-theme.json"
            }
        ]
    }
}
